    ldr r1,[sp]
    ldr r2,[sp,#4]
    bl 0x0806A674
    pop {r2-r3,pc}
pc_get_attr2_return_0:
    mov r0, #0
    pop {r2-r3,pc}
	
.org 0x080D1D74  //pc_set_attr:
